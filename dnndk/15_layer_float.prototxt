name: "15_layer"

layer {
    include { phase: TRAIN }
    type: "HDF5Data"
    top: "data" # same name as given in create_dataset!
    top: "landmarks_98"
    hdf5_data_param {
        source: "/home/dehim/Downloads/datasets/landmark_h5/train_aug.txt" # do not give the h5 files directly, but the list.
        batch_size: 25
    }
}

#######################################################
#Layer 1
#######################################################

layer {
    name: "Conv1"
    type: "Convolution"
    bottom: "data"
    top: "Conv1"
    convolution_param {
        num_output: 8
        pad: 1
        kernel_size: 3
	    dilation: 1
        stride: 1
    }
}

layer {
  name: "ReLU1"
